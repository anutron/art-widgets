{

	"Base": {

		"ART.Widget": {
			"deps": ["ART", "Keyboard", "Event.Latch"],
			"desc": "Barebones ART Widget."
		}

	},

	"Paint": {
		
		"ART.Sheet": {
			"deps": ["slickparser"]
		},
		
		"ART.Glyphs": {
			"deps": ["ART"],
			"desc": "Adds font drawing support (thanks to Cuf√≥n), and implements the text method to ART.Paint."
		}

	},

	"Utilities": {
		"Event.Latch": {
			"deps": ["Class.Extras", "Element.Event", "Class.Refactor"]
		},
		"Stacker": {
			"deps": ["Class", "Element.Event", "Element.Style"],
			"desc": "A z-index manager."
		}
	},
	
	"Widgets": {

		"ART.Browser": {
			"deps": ["ART.Window", "ART.History"],
			"desc": "A Window with basic navigation controls."
		},
		
		"ART.Button": {
			"deps": ["ART.Widget", "ART.SVG", "ART.VML", "ART.Shapes", "ART.Path", "ART.Font", "ART.Sheet", "Element.Event", "Element.Style", "Moderna", "Moderna.Bold", "Touch"],
			"desc": "Button widget."
		},

		"ART.Button.Nav": {
			"deps": ["ART.Button"],
			"desc": "Button widget with left and right arrows."
		},

		"ART.History": {
			"deps": ["ART.Button.Nav", "Element.Dimensions", "Element.Forms", "Element.Shortcuts", "ART.Glyphs"],
			"desc": "A history UI widget."
		},

		"ART.SplitView": {
			"deps": ["ART.Widget", "ART.Sheet", "Element.Event", "Element.Style", "Touch", "Fx", "Fx.Tween"],
			"desc": "Window widget."
		},

		"ART.StickyWin": {
			"deps": ["ART.Widget", "Fx.Tween", "IframeShim", "Element.Position",  "Stacker", "Touch", "Mask"],
			"desc": "Basic Window widget."
		},

		"ART.Window": {
			"deps": ["ART.Button", "ART.StickyWin", "Fx.Morph", "Moderna", "Moderna.Bold", "ART.Glyphs"],
			"desc": "Stylized window widget."
		},

		"ART.Alerts": {
			"deps": ["ART.Window"],
			"desc": "Stylized window widget."
		},

		"ART.SolidWindow": {
			"deps": ["ART.Window"],
			"desc": "Stylized solid window widget."
		}

		
	}

}
